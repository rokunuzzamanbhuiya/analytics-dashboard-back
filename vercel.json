{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node@3.0.7"
        }
    ],
    "functions": {
        "api/server.js": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.js"
        }
    ]
}